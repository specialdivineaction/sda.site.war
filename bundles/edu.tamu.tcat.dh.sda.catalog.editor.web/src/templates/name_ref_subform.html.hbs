<fieldset>
    <legend>Name</legend>

    <div class="form-group">
        <label for="name">Name</label>
        <input class="form-control" type="text" name="name" id="name">
    </div>

    <div class="row">
        <div class="col-sm-1">
            <div class="form-group">
                <label for="title">Title</label>
                <input class="form-control" type="text" name="title" id="title" value="{{title}}">
            </div>
        </div>
        <div class="col-sm-10">
            <div class="row">
                <div class="col-xs-4">
                    <div class="form-group">
                        <label for="givenName">Given</label>
                        <input class="form-control" type="text" name="givenName" id="givenName" value="{{givenName}}">
                    </div>
                </div>
                <div class="col-xs-2">
                    <div class="form-group">
                        <label for="middleName">Middle</label>
                        <input class="form-control" type="text" name="middleName" id="middleName" value="{{middleName}}">
                    </div>
                </div>
                <div class="col-xs-4">
                    <div class="form-group">
                        <label for="familyName">Family</label>
                        <input class="form-control" type="text" name="familyName" id="familyName" value="{{familyName}}">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-1">
            <div class="form-group">
                <label for="suffix">Suffix</label>
                <input class="form-control" type="text" name="suffix" id="suffix" value="{{suffix}}">
            </div>
        </div>
    </div>
</fieldset>
